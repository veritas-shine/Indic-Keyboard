<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <!-- MYANMAR LETTER JHA -->
            <Key
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio"
                latin:keySpec="0x16F00" />
            <!-- MYANMAR LETTER WA -->
            <Key
                latin:keyLabelFlags="fontNormal"
                latin:keySpec="0x16F01" />
            <!-- MYANMAR LETTER I -->
            <Key
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio"
                latin:keySpec="0x16F02" />
            <!-- MYANMAR LETTER I -->
            <Key
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio"
                latin:keySpec="0x16F03" />
            <!-- MYANMAR SYMBOL AFOREMENTIONED -->
            <Key
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio"
                latin:keySpec="0x16F04" />
            <!-- MYANMAR LETTER II -->
            <Key
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio"
                latin:keySpec="0x16F05" />
            <!-- MYANMAR LETTER U -->
            <Key
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio"
                latin:keySpec="0x16F06" />
            <!-- MYANMAR SYMBOL COMPLETED -->
            <Key
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio"
                latin:keySpec="0x16F07" />
            <!-- MYANMAR LETTER GREAT SA -->
            <Key
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio"
                latin:keySpec="0x16F08" />

            <!-- MYANMAR LETTER AU -->
            <Key
                latin:keyHintLabel=")"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio|autoXScale"
                latin:keySpec="0x16F0B"
                latin:moreKeys=")" />
        </case>

        <default>
            <!-- MYANMAR LETTER CHA -->
            <Key
                latin:keyLabelFlags="fontNormal"
                latin:keySpec="0x16F16" />
            <!-- MYANMAR LETTER TA
                 MYANMAR LETTER TTA -->
            <Key
                latin:keyLabelFlags="fontNormal"
                latin:keySpec="0x16F01" />
            <!-- MYANMAR LETTER NA -->
            <Key
                latin:keyHintLabel="0x16F2E"
                latin:keyLabelFlags="fontNormal"
                latin:keySpec="0x16F2D"
                latin:moreKeys="0x16F2E" />
            <!-- MYANMAR LETTER MA -->
            <Key
                latin:keyHintLabel="0x16F36"
                latin:keyLabelFlags="fontNormal"
                latin:keySpec="0x16F35"
                latin:moreKeys="0x16F36" />
            <!-- MYANMAR LETTER A -->
            <Key
                latin:keyHintLabel="0x16F0B"
                latin:keyLabelFlags="fontNormal"
                latin:keySpec="0x16F0A"
                latin:moreKeys="0x16F0B" />
            <!-- MYANMAR LETTER PA -->
            <Key
                latin:keyHintLabel="0x16F44"
                latin:keyLabelFlags="fontNormal"
                latin:keySpec="0x16F43"
                latin:moreKeys="0x16F44" />
            <!-- MYANMAR LETTER KA -->
            <Key
                latin:keyHintLabel="0x16F6C"
                latin:keyLabelFlags="fontNormal"
                latin:keySpec="0x16F6A"
                latin:moreKeys="0x16F6B" />
            <!-- MYANMAR LETTER NGA -->
            <Key
                latin:keyHintLabel="0x16F63"
                latin:keyLabelFlags="fontNormal"
                latin:keySpec="0x16F61"
                latin:moreKeys="0x16F62" />
            <!-- MYANMAR LETTER SA -->
            <Key
                latin:keyHintLabel="0x16F5B"
                latin:keyLabelFlags="fontNormal"
                latin:keySpec="0x16F59"
                latin:moreKeys="0x16F5A" />
            <!-- MYANMAR LETTER O -->
            <Key
                latin:keyHintLabel="0x16F01"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio"
                latin:keySpec="0x16F00"
                latin:moreKeys="0x16F01" />
        </default>
    </switch>
</merge>
